{
  "name": "solidjs-show-code",
  "version": "0.1.1",
  "description": "Code viewer for solidJs",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "dist/esm/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "solid": "./dist/source/index.js",
      "import": "./dist/esm/index.js",
      "browser": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "node": "./dist/cjs/index.js"
    }
  },
  "jest": {
    "preset": "solid-jest/preset/browser",
    "setupFilesAfterEnv": [
      "<rootDir>/jest-setup.ts"
    ]
  },
  "scripts": {
    "prepack": "yarn build",
    "build": "rollup -c",
    "watch": "rollup -cw"
  },
  "keywords": [
    "solidJs",
    "code",
    "viewer"
  ],
  "author": "Mark Gregg <gregg.mark@gmail.com> (https://github.com/markgregg)",
  "license": "MIT",
  "peerDependencies": {
    "solid-js": "^1.6.9"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@testing-library/jest-dom": "^5.16.5",
    "@types/jest": "^29.4.0",
    "babel-jest": "^29.4.1",
    "babel-preset-jest": "^29.4.0",
    "babel-preset-solid": "^1.6.10",
    "jest": "^29.4.1",
    "jest-environment-jsdom": "^29.4.1",
    "jsdom": "^21.1.0",
    "prettier": "2.8.3",
    "regenerator-runtime": "^0.13.11",
    "solid-jest": "^0.2.0",
    "solid-testing-library": "^0.5.1",
    "autoprefixer": "^10.4.13",
    "rollup": "^3.10.1",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-preset-solid": "^2.0.1",
    "solid-js": "^1.6.9",
    "typescript": "^4.9.4",
    "vite": "^4.0.4",
    "vite-plugin-solid": "^2.5.0"
  }
}
